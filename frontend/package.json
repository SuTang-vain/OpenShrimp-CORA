{
  "name": "shrimp-agent-frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Shrimp Agent V2 - Modern React Frontend",
  "author": "Shrimp Team",
  "license": "MIT",
  "scripts": {
